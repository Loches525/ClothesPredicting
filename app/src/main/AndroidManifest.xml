<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.clothespredicting">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.WeatherActivity"></activity>
        <activity android:name=".activity.ChooseAreaActivity" ></activity>

        <service android:name="com.example.android.clothespredicting.service.AutoUpdateService" ></service>
        <receiver android:name="com.example.android.clothespredicting.receiver.AutoUpdateReceiver"></receiver>
    </application>

</manifest>
